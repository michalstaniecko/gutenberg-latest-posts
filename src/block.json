{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blocks-course/latest-posts",
	"version": "0.1.0",
	"title": "Blocks course - latest posts",
	"category": "widgets",
	"icon": "admin-post",
	"keywords": [
		"latest",
		"posts"
	],
	"description": "Blocks course - Latest posts.",
	"supports": {
		"html": false
	},
	"textdomain": "latest-posts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"numberOfPosts": {
			"type": "number",
			"default": 5
		},
		"displayFeaturedImage": {
			"type": "boolean",
			"default": true
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderBy": {
			"type": "string",
			"default": "date"
		}
	}
}
